# ClickHouse Configuration
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9000
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=your_clickhouse_password
CLICKHOUSE_DATABASE=default

# GCS Configuration
GCS_BUCKET_NAME=your-bucket-name
GCS_SERVICE_ACCOUNT_KEY=/path/to/your/service-account-key.json

# Watch Directory (Linux specific)
WATCHED_FOLDER=/var/www/html/pmen/public/documents

# Logging
LOG_LEVEL=INFO
LOG_FILE=/var/log/pmen-sync/pmen_sync.log

# Processing
BATCH_SIZE=50
PROCESSING_INTERVAL=300  # seconds between processing runs
